<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.ui.theme" value="simple"/>
	<constant name="struts.devMode" value="true" />
	<constant name="struts.i18n.encoding" value="UTF-8" />

	<package name="sysback" namespace="/sysback" extends="default">
		<action name="sysbackAction_*" method="{1}" class="sysbackAction">
			<result name="tosysbackview">/WEB-INF/pages/start/sysback/sysback.jsp</result>
			<result name="sendbackmail" >/WEB-INF/pages/start/sysback/sysbacksuccess.jsp</result>
		</action>
		
	</package>

</struts>
	